<table>
  <tbody>
    <tr>
      <th><%= t(:wiki_menu_name) %></th>
      <td>
        <input type="text" id="settings_wiki_menu_name" name="settings[wiki_menu_name" value="<%= settings['wiki_menu_name'] %>" >
      </td>
    </tr>
    <tr>
      <th><%= t(:redirect_to_wiki_project) %></th>
      <td>
        <%= select_tag 'settings[redirect_to_wiki_project]', options_from_collection_for_select(Project.all,:identifier,:name, settings['redirect_to_wiki_project']), :include_blank => true, :multiple=> false  %>
      </td>
    </tr>
  </tbody>
</table>